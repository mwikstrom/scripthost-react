    "react-test-renderer": "^17.0.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.64.0",
    "scripthost": "^0.3.11",
    "scripthost-browser": "^0.1.14",
    "scripthost-core": "^0.2.6",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.4.0",
