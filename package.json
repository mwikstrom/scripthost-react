{
  "name": "scripthost-react",
  "version": "0.1.10",
  "description": "React bindings for Scripthost",
  "main": "./dist/scripthost-react.js",
  "typings": "./dist/scripthost-react.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "rimraf temp lib && npm run lint && tsc",
